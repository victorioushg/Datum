              <DbSource ConnectionRef="ConnectionString (MySettings)" DbObjectType="Unknown" FillMethodModifier="Public" FillMethodName="Fill" GenerateMethods="Both" GenerateShortCommands="false" GeneratorGetMethodName="GetData" GeneratorSourceName="Fill" GetMethodModifier="Public" GetMethodName="GetData" QueryType="Rowset" ScalarCallRetval="System.Object, mscorlib, Version=2.0.0.0, Culture=neutral, PublicKeyToken=b77a5c561934e089" UseOptimisticConcurrency="true" UserGetMethodName="GetData" UserSourceName="Fill">
                <SelectCommand>
                  <DbCommand CommandType="Text" ModifiedByUser="true">
                    <CommandText>SELECT     a.CODCON, c.DESCRIPCION, b.FECHASI, a.ASIENTO, a.referencia, a.concepto, date_format(b.FECHASI, '%b/%Y') AS mes, d.saldo, 0.00 AS debitos, 
                      0.00 AS creditos
FROM         jscotrenasi a LEFT OUTER JOIN
                      jscotencasi b ON a.ASIENTO = b.ASIENTO AND a.actual = b.actual AND a.EJERCICIO = b.EJERCICIO AND a.ID_EMP = b.ID_EMP LEFT OUTER JOIN
                      jscotcatcon c ON a.CODCON = c.CODCON AND a.ID_EMP = c.ID_EMP LEFT OUTER JOIN
                          (SELECT     a.CODCON, SUM(a.IMPORTE) AS saldo, a.EJERCICIO, a.ID_EMP
                            FROM          jscotrenasi a LEFT OUTER JOIN
                                                   jscotencasi b ON a.ASIENTO = b.ASIENTO AND a.actual = b.actual AND a.EJERCICIO = b.EJERCICIO AND a.ID_EMP = b.ID_EMP
                            WHERE      (b.FECHASI &lt; '1/15/2009') AND (a.actual = 1) AND (a.EJERCICIO = '') AND (a.ID_EMP = '01')
                            GROUP BY a.CODCON) d ON a.CODCON = d.CODCON AND a.EJERCICIO = d.EJERCICIO AND a.ID_EMP = d.ID_EMP
WHERE     (b.FECHASI &gt;= '1/15/2009') AND (b.FECHASI &lt;= '1/15/2009') AND (a.actual = 1) AND (a.EJERCICIO = '') AND (a.ID_EMP = '01')
ORDER BY a.CODCON, b.FECHASI, a.ASIENTO</CommandText>
                    <Parameters />
                  </DbCommand>
                </SelectCommand>
              </DbSource>
            </MainSource>
