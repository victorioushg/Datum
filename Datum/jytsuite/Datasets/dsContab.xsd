          <TableAdapter BaseClass="System.ComponentModel.Component" DataAccessorModifier="AutoLayout, AnsiClass, Class, Public" DataAccessorName="dtMayorAnaliticoTableAdapter" GeneratorDataComponentClassName="dtMayorAnaliticoTableAdapter" Name="dtMayorAnalitico" UserDataComponentName="dtMayorAnaliticoTableAdapter">
