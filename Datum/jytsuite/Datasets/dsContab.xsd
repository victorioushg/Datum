                <SelectCommand>
                  <DbCommand CommandType="Text" ModifiedByUser="true">
                    <CommandText>select a.asiento, a.fechasi, a.descripcion, a.debitos total_debitos, a.creditos total_creditos, b.codcon, c.descripcion nombre, b.referencia, b.concepto, 0.00 debitos,  0.00 creditos, 'xxxxx'  regla, 'x' signo
from jscotencasi a  
left join jscotrenasi b on 
   (a.asiento = b.asiento and a.actual = b.actual and a.ejercicio = b.ejercicio and a.id_emp = b.id_emp) left join jscotcatcon c on (b.codcon = c.codcon and b.id_emp = c.id_emp ) 
        Where
a.actual = 0 and a.ejercicio = '' and a.id_emp = '01'</CommandText>
                    <Parameters />
                  </DbCommand>
                </SelectCommand>
              </DbSource>
            </MainSource>
